body
  color: body-color
  background: body-background
  font-family: tahoma,arial,"microsoft yahei",sans-serif
  padding: 0 !important
  margin: 0 !important
  font-size:12px !important
h1
h2
h3
h4
h5
label
  font-weight: 300
ul li
  list-style: none
a
&:hover
&:focus
  text-decoration: none
  outline: none
  outline: 0
*:focus
  outline: none
table tr th
  color: normal-color
::selection
::-moz-selection
  background: normal-color
  color: body-background
.panel
  padding: 5px 15px
  border: 1px solid #d5d5d5
  border-radius:2px
.panel-heading
  border-bottom: 1px solid #e5e5e5
.wrapper
  display: inline-block
  padding: 15px
  width: 100%
.code-box
  margin: auto 10px !important