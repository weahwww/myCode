body
  color: body-color
  background: body-background
  font-family: tahoma,arial,"microsoft yahei",sans-serif
  padding: 0 !important
  margin: 0 !important
  font-size:12px !important
h1
h2
h3
h4
h5
label
  font-weight: 300
ul li
  list-style: none
a
&:hover
&:focus
  text-decoration: none
  outline: none
  outline: 0
*:focus
  outline: none
table tr th
  color: normal-color
::selection
::-moz-selection
  background: normal-color
  color: body-background
.wrapper
  display: inline-block
  width: 100%
.code-box
  margin: auto 6px !important